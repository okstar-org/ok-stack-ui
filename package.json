{
  "name": "ok-stack-ui",
  "version": "24.09.02",
  "engines": {
    "node": ">=16.0.0 <20.0.0",
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "postinstall": "node git-version.js",
    "start": "ng serve",
    "test": "ng test",
    "e2e": "ng e2e",
    "build": "npm run postinstall && ng build",
    "build:release": "npm run postinstall && ng build --configuration=production",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "hmr": "ng serve --hmr",
    "deploy": "ng deploy",
    "release": "npm run build:release",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.11",
    "@angular/cdk": "^16.2.10",
    "@angular/common": "^16.2.11",
    "@angular/compiler": "^16.2.11",
    "@angular/core": "^16.2.11",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.11",
    "@angular/material": "^16.2.10",
    "@angular/material-moment-adapter": "^16.2.10",
    "@angular/platform-browser": "^16.2.11",
    "@angular/platform-browser-dynamic": "^16.2.11",
    "@angular/router": "^16.2.11",
    "@ng-matero/extensions": "^16.1.2",
    "@ng-matero/extensions-moment-adapter": "^16.0.0",
    "@ngx-formly/core": "^6.2.2",
    "@ngx-formly/material": "^6.2.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@w11k/git-info": "^1.1.2",
    "apexcharts": "^3.26.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "ng-matero": "^16.1.3",
    "ngx-logger": "^5.0.12",
    "ngx-permissions": "^16.0.1",
    "ngx-progressbar": "^11.1.0",
    "ngx-toastr": "^17.0.2",
    "photoviewer": "^3.8.1",
    "screenfull": "^6.0.0",
    "tslib": "^2.6.0",
    "xlsx": "^0.18.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.8",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "^16.2.0",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "^16.2.8",
    "@angular/compiler-cli": "~16.2.11",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "fs-extra": "6.0.1",
    "git-describe": "4.1.1",
    "git-revision-webpack-plugin": "^5.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
